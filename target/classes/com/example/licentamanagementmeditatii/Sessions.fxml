      <TableView fx:id="table_materie" layoutX="230.0" layoutY="33.0" onMouseClicked="#getSelected" prefHeight="486.0" prefWidth="647.0" style="-fx-border-radius: 10; -fx-background-radius: 10; -fx-border-color: transparent;">
        <columns>
          <TableColumn fx:id="column_id" prefWidth="33.0" text="ID" />
          <TableColumn fx:id="column_materie" prefWidth="81.0" text="Materie" />
