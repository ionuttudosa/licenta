      <Button fx:id="button_delete" layoutX="38.0" layoutY="494.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="135.0" style="-fx-background-color: red;" text="Stergere" />
      <Button fx:id="button_update" layoutX="36.0" layoutY="454.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="135.0" style="-fx-background-color: yellow;" text="Actualizare" />
      <Button fx:id="button_add" layoutX="38.0" layoutY="417.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="135.0" style="-fx-background-color: green;" text="Adaugare" />
