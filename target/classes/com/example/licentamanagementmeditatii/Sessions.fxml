            <TableColumn fx:id="column_student" prefWidth="122.0" text="Student" />
            <TableColumn fx:id="column_pret_per_ora" prefWidth="83.0" text="pret per ora" />
            <TableColumn fx:id="column_durata" prefWidth="54.0" text="durata" />
            <TableColumn fx:id="column_data" prefWidth="119.0" text="data" />
            <TableColumn fx:id="column_pret_total" prefWidth="69.0" style="fx-background-radius: 0.0 5.0 0.0 0.0;" text="pret total " />
